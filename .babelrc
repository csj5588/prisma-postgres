{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@/components": "./components",
          "@/prisma": "./lib/prisma",
          "@/pages": "./pages",
          "@/views": "./views",
          "@/styles": "./styles",
          "@/constant": "./constant",
          "@/utils": "./utils"
        }
      }
    ],
    [
      "import",
      {
        "libraryName": "antd"
      }
    ]
  ]
}